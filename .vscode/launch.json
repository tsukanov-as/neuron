    "version": "0.2.0",
    "configurations": [
        {
            "name": "Golang: Current File",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${file}",
            "debugAdapter": "legacy",
        },
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "internalConsole",
            "internalConsoleOptions": "openOnSessionStart",
            "justMyCode": true,
        }
    ]
}